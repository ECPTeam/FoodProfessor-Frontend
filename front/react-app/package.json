    "eslint-config-prettier": "^8.3.0",
    "husky": "^4.3.8",
    "lint-staged": "^11.0.0",
    "postcss": "^7.0.36",
    "prettier": "^2.3.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
